#include <iostream>
#include <cmath>
using namespace std;
void task60()
{
	cout << "Задание№60";
	double РасстояниеОКМ = 0, РасстояниеФТ = 0, РасстояниеДКМ = 0;
	cout << "\n\nВведите расстояние №1(км):";
	cin >> РасстояниеОКМ;
	cout << "\n\nВведите расстояние №2(футы):";
	cin >> РасстояниеФТ;
	РасстояниеДКМ = РасстояниеФТ * 0.31;
	cout << "\n\nРасстояние №1 =" << РасстояниеОКМ << "км.\n\nРасстояние №2 =" << РасстояниеФТ << "футов \n\nКакое расстояние больше?";
	if (РасстояниеОКМ < РасстояниеДКМ)
	{
		cout << "\n\nРасстояние №2 больше ";
	}
	else if (РасстояниеОКМ > РасстояниеДКМ)
	{
		cout << "\n\nРасстояние №1 больше ";
	}
	cout << "\n\n\n\n";
}
void task73()
{
	cout << "Задание№73";
	double abc = 0, a = 0, b = 0, c = 0, z = 0, x = 0;
	cout << "\n\nЗадайте число abc: ";
	cin >> abc;
	cout << "\n\nДано число " << abc << "\n\nОпределить, равен ли квадрат этого числа сумме кубов его цифр.";
	z = (a * a * a) + (b * b * b) + (c * c * c);
	x = abc * abc;
	if (x == abc)
	{
		cout << "\n\nКвадрат числа равен сумме кубов его цифр";
	}
	else
	{
		cout << "\n\nКвадрат числа не равен сумме кубов его цифр";
	}
	cout << "\n\n\n";
}
void task84()
{
	cout << "Задание№84";
	double m = 0, n = 0, z = 0;
	cout << "\n\nm=";
	cin >> m;
	cout << "\n\nn=";
	cin >> n;
	cout << "\n\nДаны целые числа m, n.\n\nЕсли числа не равны, то заменить каждое из них одним и тем же числом, равным большему из исходных, а если равны, то заменить числа нулями.";
	if (m > n)
	{
		cout << "\n\nm=" << m;
		cout << "\n\nn=" << m;
	}
	else if (m < n)
	{
		cout << "\n\nm=" << n;
		cout << "\n\nn=" << n;
	}
	else if (m == n)
	{
		cout << "\n\nm=0";
		cout << "\n\nn=0";
	}
	cout << "\n\n\n";
}
void task95()
{
	cout << "Задание№95";
	double t = 0;
	cout << "\n\nТемпература в помещении =";
	cin >> t;
	if (t > 60)
	{
		cout << "\nПожароопасная ситуация";
	}
	cout << "\n\n\n";
}
void task106()
{
	cout << "Задание№106";
	int x, y;
	cout << "\n\nВведите число x:";
	cin >> x;
	cout << "\n\nВведите число y:";
	cin >> y;
	cout << x << " " << (x % 2 == 0 ? "четное" : "нечетное") << endl;
	cout << y << " " << (y % 2 == 0 ? "четное" : "нечетное") << endl;
	cout << "\n\n\n";
}
void task117()
{
	cout << "Задание№117";
	cout << "\n\nax^3 + bx = 0";
	double x = 0, a = 0, b = 0, z = 0;
	cout << "\n\nВведите а:";
	cin >> a;
	cout << "\n\nВведите b:";
	cin >> b;
	z = (a * x * x * x) + (b * x);
	cout << "x=" << x;
	cout << "\n\n\n";
}
void task128()
{
	cout << "Задание№128";
	int x = 0;
	cout << "\n\nВведите пароль:";
	cin >> x;
	if (x == 9583 || x == 1747)
	{
		cout << "\n\nДоступны модули баз А, В, С";
	}
	else if (x == 3331 || x == 7922)
	{
		cout << "\n\nдоступны модули баз В, С";
	}
	else if (x == 9455 || x == 8997)
	{
		cout << "\n\nдоступен модуль базы С";
	}
	else
	{
		cout << "\n\nотказоно в доступе";
	}
	cout << "\n\n\n";
}
void task139()
{
	double a = 0, b = 0, c = 0;
	cout << "Задание№139";
	cout << "\n\nОпределить, является ли треугольник со сторонами a, b, с равнобедренным";
	cout << "\n\nВведите a:";
	cin >> a;
	cout << "\n\nВведите b:";
	cin >> b;
	cout << "\n\nВведите c:";
	cin >> c;
	if (a = b)
	{
		cout << "\n\nТреугольник равнобедренный";
	}
	else if (a = c)
	{
		cout << "\n\nТреугольник равнобедренный";
	}
	else if (b = c)
	{
		cout << "\n\nТреугольник равнобедренный";
	}
	else if (b = c)
	{
		cout << "\n\nТреугольник равнобедренный";
	}
	cout << "\n\n\n";
}
void task150()
{
	cout << "Задание№150";
	int a = 0, b = 0, e = 0, d = 0, S1 = 0, S2 = 0;
	double z = 0;
	cout << "\n\nИмеются стол прямоугольной формы с размерами a * b (а и b — ЦЕЛЫЕ ЧИСЛА, a > b) и кости домино с размерами с * d * e (c, d и e — ЦЕЛЫЕ ЧИСЛА, с > d > e).\n\nНайти вариант размещения на столе наибольшего количества костей.\n\nВсе размещаемые кости должны лежать на одной и той же грани в один ярус и не свешиваться со стола.\n\nВсе ребра костей домино должны быть параллельны или перпендикулярны каждой стороне стола.";
	cout << "\n\nТак как с > d > e, то кости на стол выгоднее всего ставить на рёбра  d  и  е";
	cout << "\n\ne=";
	cin >> e;
	cout << "\n\nd=";
	cin >> d;
	cout << "\n\na=";
	cin >> a;
	cout << "\n\nb=";
	cin >> b;
	S1 = a * b;
	S2 = d * e;
	if (a < b || d < e)
	{
		cout << "Неверно введены данные";
	}
	else
	{
		z = S1 / S2;
		floor(z);
		cout << "На столе вмещается " << z << " кости(ей)";
	}
	cout << "\n\n\n";
}
int main()
{
	setlocale(LC_ALL, "Russian");
	task60();
	task73();
	task84();
	task95();
	task106();
	task117();
	task128();
	task139();
	task150();
}
